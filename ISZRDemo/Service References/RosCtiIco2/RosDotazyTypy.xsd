    <xs:sequence>
      <xs:element minOccurs="0" name="DatumUkonceniCinnostiUdaj" nillable="true" type="edi:DatumUkonceniCinnostiStavType" />
      <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FoResponseType">
    <xs:annotation>
      <xs:documentation>
                Fyzická osoba (obyvatel) s vazbou na statutární orgán nebo osobu v odpovědi z ROS 
                včetně stavu jednotlivých údajů a provozních údajů o poslední změně.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="edi:FoStavType">
        <xs:sequence>
          <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FoResponse2Type">
    <xs:annotation>
      <xs:documentation>
                Fyzická osoba (obyvatel) s vazbou na statutární orgán nebo osobu v odpovědi z ROS 
                včetně stavu jednotlivých údajů a provozních údajů o poslední změně.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ros:FoStav2Type">
        <xs:sequence>
          <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AdresaResponseType">
    <xs:annotation>
      <xs:documentation>
                Adresa v odpovědi z ROS včetně stavu a provozních údajů o poslední změně.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="edi:AdresaStavType">
        <xs:sequence>
          <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AdresaSidlaStavResponseType">
    <xs:annotation>
      <xs:documentation>
                Adresa sídla v odpovědi z ROS včetně stavu, který může být i nedefinovaný.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ros:AdresaType">
        <xs:attribute name="stav" type="ros:StavRosType" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AdresaSidlaResponseType">
    <xs:annotation>
      <xs:documentation>
                Adresa sídla v odpovědi z ROS včetně stavu a a provozních údajů o poslední změně.
                Vrácení prázného údaje adresy je možné jen v případě hodnoty stavu = nedefinovany.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AdresaSidlaUdaj" nillable="true" type="AdresaSidlaStavResponseType" />
      <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdresaDorucovaciResponseType">
    <xs:annotation>
      <xs:documentation>
                Adresa doručovací v odpovědi z ROS včetně stavu a a provozních údajů o poslední změně.
