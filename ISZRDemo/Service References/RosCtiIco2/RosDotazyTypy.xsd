<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:edi="urn:cz:isvs:ros:schemas:RosEditaceTypy:v1" xmlns:ros="urn:cz:isvs:ros:schemas:RosTypy:v2" xmlns:reg="urn:cz:isvs:reg:schemas:RegTypy:v1" xmlns="urn:cz:isvs:ros:schemas:RosDotazyTypy:v1" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:cz:isvs:ros:schemas:RosDotazyTypy:v1" version="2.4.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="../../RegTypy.xsd" namespace="urn:cz:isvs:reg:schemas:RegTypy:v1" />
  <xs:import schemaLocation="RosTypy.xsd" namespace="urn:cz:isvs:ros:schemas:RosTypy:v2" />
  <xs:import schemaLocation="RosEditaceTypy.xsd" namespace="urn:cz:isvs:ros:schemas:RosEditaceTypy:v1" />
  <xs:annotation>
    <xs:documentation xml:lang="cs">Návrh XML schématu s definicemi společných datových prvků IS ROS.
          Toto schéma obsahuje definice základních datových prvků používaných pro dotazy a odpovědi na dotazy
          do registru osob (ROS).
        </xs:documentation>
  </xs:annotation>
  <xs:simpleType name="TypZmenyType">
    <xs:annotation>
      <xs:documentation>
                Typ změny údajů osoby: I=insert (vložení); U=update (oprava); D=delete (zrušení),
                V=všechny typy změny.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="1" />
      <xs:enumeration value="I">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Insert</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="U">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Update</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="D">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Delete</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="V">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Všechny</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PravniStavStavResponseType">
    <xs:annotation>
      <xs:documentation>                
                Právní stav v odpovědi z ROS včetně stavu.            
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="KodPravnihoStavu" type="ros:KodPravnihoStavuType" />
      <xs:element minOccurs="0" name="NazevPravnihoStavu" type="ros:NazevPravnihoStavuType" />
    </xs:sequence>
    <xs:attribute name="stav">
      <xs:simpleType>
        <xs:restriction base="ros:StavRosType">
          <xs:enumeration value="spravny" />
          <xs:enumeration value="nespravny" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="PravniStavResponseType">
    <xs:annotation>
      <xs:documentation>                
                Právní stav v odpovědi z ROS včetně stavu a provozních údajů o poslední změně.            
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="PravniStavStavResponseType">
        <xs:sequence>
          <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PravniFormaStavResponseType">
    <xs:annotation>
      <xs:documentation>
                Právní forma v odpovědi z ROS včetně stavu.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="KodPravniFormy" type="ros:KodPravniFormyType" />
      <xs:element minOccurs="0" name="NazevPravniFormy" type="ros:NazevPravniFormyType" />
    </xs:sequence>
    <xs:attribute name="stav">
      <xs:simpleType>
        <xs:restriction base="ros:StavRosType">
          <xs:enumeration value="spravny" />
          <xs:enumeration value="nespravny" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="PravniFormaResponseType">
    <xs:annotation>
      <xs:documentation>
                Právní forma v odpovědi z ROS včetně stavu a provozních údajů o poslední změně.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="PravniFormaStavResponseType">
        <xs:sequence>
          <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="NazevOsobyResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">
                Název osoby v odpovědi z ROS včetně stavu a provozních údajů o poslední změně. 
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="NazevOsobyUdaj" type="edi:NazevOsobyStavType" />
      <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DatovaSchrankaResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">
                Datová schránka v odpovědi z ROS včetně stavu a provozních údajů o poslední změně. 
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DatovaSchrankaUdaj" type="edi:DatovaSchrankaStavType" />
      <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DatoveSchrankyResponseType">
    <xs:annotation>
      <xs:documentation>
                Datové schránky osoby v odpovědi z ROS včetně stavu a provozních údajů
                o poslední změně jednotlivých datových schránek.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="DatovaSchranka" type="DatovaSchrankaResponseType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DatoveSchrankyStavResponseType">
    <xs:annotation>
      <xs:documentation>
                Datové schránky osoby v odpovědi z ROS včetně stavu jednotlivých datových schránek.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="DatovaSchranka" type="edi:DatovaSchrankaStavType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DatumVznikuOpravneniResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">
                Datum vzniku oprávnění k činnosti osoby v agendě v odpovědi z ROS včetně stavu
                a provozních údajů o poslední změně.
                Vrácení prázného údaje je možné jen v případě hodnoty stavu = nedefinovany.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DatumVznikuOpravneniUdaj" nillable="true" type="DatumVznikuOpravneniStavResponseType" />
      <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DatumVznikuOpravneniStavResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">
                Datum vzniku oprávnění k činnosti osoby v agendě v odpovědi z ROS včetně stavu. 
            </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="ros:DatumVznikuOpravneniType">
        <xs:attribute name="stav" type="ros:StavRosType" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="DatumZanikuOpravneniResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">
                Datum zániku oprávnění k činnosti osoby v agendě v odpovědi z ROS včetně stavu
                a provozních údajů o poslední změně.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="DatumZanikuOpravneniUdaj" nillable="true" type="DatumZanikuOpravneniStavResponseType" />
      <xs:element name="ProvozniUdaje" type="ros:UdajeZmenyType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DatumZanikuOpravneniStavResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">
                Datum zániku oprávnění k činnosti osoby v agendě v odpovědi z ROS včetně stavu. 
            </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="ros:DatumZanikuOpravneniType">
