      <xs:enumeration value="AifoCilNeplatne">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Cílové AIFO není platné. Pro převod není důležité, zda bylo sloučeno, zrušeno apod.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="StavOvereniPrvkuRuianType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Stav adresního prvku v RUIAN pro ověření / načtení z RUIAN.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="existuje" />
      <xs:enumeration value="nenalezen" />
      <xs:enumeration value="zrusen" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TypPrvkuRuianType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Typy referenčního údaje RUIAN</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="PA">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Pozemek v podobě parcely</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SO">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Stavební objekt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AD">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Adresní místo</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="IdentifikatorRuianType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Identifikátor prvku RUIAN včetně jeho typu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="KodPrvku" type="reg:IdentifikatorType" />
      <xs:element name="TypPrvku" type="reg:TypPrvkuRuianType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SeznamPrvkuType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Seznam identifikátorů RUIAN neuvedených v SeznamIdAdres. Slouží pro kontrolu existence v RUIAN.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="OdkazRuian">
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="reg:IdentifikatorRuianType">
              <xs:attribute name="stavOvereniPrvku" type="reg:StavOvereniPrvkuRuianType">
                <xs:annotation>
                  <xs:documentation xml:lang="cs">Informace o výsledku načtení v RUIAN, pokud se nepodařilo načíst.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="MetodaKompreseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Metoda komprese dat.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="zadna" />
      <xs:enumeration value="gzip" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="BinarniDataType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Binární data MTOM/XOP.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="Base64Data" type="xs:base64Binary" />
      <xs:element name="Include" type="xop:Include" />
    </xs:choice>
    <xs:attribute name="typ" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="KomprimovanaDataType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Komprimovaná data MTOM/XOP.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="reg:BinarniDataType">
        <xs:attribute name="metoda" type="reg:MetodaKompreseType" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AsyncDotazDataResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Univerzální asynchronní odpověd na dotaz.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##any" processContents="lax" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SouborType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Soubor na rozhraní.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IdSouboru" type="reg:UuidType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Identifikátor souboru na rozhraní (název souboru bez přípony, soubor je na rozhraní pro stažení výstavován bez přípony).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Heslo" type="xs:base64Binary">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Heslo pro předávaný soubor, pokud je zaheslován. Způsob tvorby hesla je dán definovaným algoritmem.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Otisk" type="reg:OtiskSHA1Type">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Otisk obsahu souboru SHA-1.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TypSouboru" type="reg:SouborTypType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Typ souboru.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UcelSouboru" type="reg:UcelSouboruTypType">
        <xs:annotation>
          <xs:documentation>Pro rozlišení významu souboru v procesu, ve kterém se používá. Obsah je definován při definici procesu. Primárně určeno pro zpracování dat na výstupu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DostupnyOd" type="reg:CasovaZnackaType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Od kdy je garantována dostupnost souboru ke stažení.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DostupnyDo" type="reg:CasovaZnackaType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Do kdy je garantována dostupnost soubroru ke staženi (s tím, že systém může omezit interně například počet stažení).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Umisteni" type="reg:NonEmptyNormStringType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">URL, na které je soubor dostupný. Zda je vyplněno je dáno procesem, ve kterém se použivá související služba, pro některé procesy nemusí být vyplněno a může být definováno na úrovni konfigurace.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="PlatnostOd" type="reg:CasovaZnackaType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Volitelně platnst souboru, v závislosti na typu procesu ve kterém je použito.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SouboryType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Seznam souborů pro předání na rozhraní.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Soubory" type="reg:SouborType" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="OtiskSHA1Type">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Otisk dat algoritmem SHA-1</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary">
      <xs:length value="20" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SouborTypType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Typ souboru, výčtový typ.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="7Z">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Komprimováno pomocí 7-zip. Obsah je dán procesem, ve kterém je použito.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="7ZAES256">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Komprimováno pomocí 7-zip. Obsah je dán procesem, ve kterém je použito. Šifrováno AES256.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="TXTUTF8">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Txt v kódování UTF-8.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ZIP">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Komprimováno pomocí ZIP, Obsah je dán procesem, ve kterém je použito.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UcelSouboruTypType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Účel typu souboru, výčtový typ. Pouze popisuje účel souboru, neříká, co má příjemce provést.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="DATA">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou obecná data procesu. Vrací AISEO / AISC / ROB pokud vrací data.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DATAPEAISC">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou data procesu. Vrací AISC / ROB pokud vrací specifická data vyžadující rozlišení.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DATAPEAISEO">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou data procesu. Vrací AISEO / ROB pokud vrací specifická data vyžadující rozlišení.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DATAAISC">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou data procesu. Vrací AISC / ROB pokud vrací specifická data vyžadující rozlišení.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DATAAISEO">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou data procesu. Vrací AISEO / ROB pokud vrací specifická data vyžadující rozlišení.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AIFOPEAISC">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou AIFO pro PE AISC. Vrací ROB (obsahem jsou AIFOrob pro PE CIS).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AIFOAISC">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou AIFO PE AISC. Vrací AISC (obsahem jsou AIFOaisc).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AIFOPEAISEO">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou AIFO PE AISEO. Vrací ROB (obsahem jsou AIFOrob pro PE AISEO).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AIFOAISEO">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou AIFO PE AISEO. Vrací AISEO (obsahem jsou AIFOaiseo)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AIFOAISEO">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou AIFO PE AISEO. Vrací AISEO (obsahem jsou AIFOaiseo)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ADRRUIAN">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Obsahem souboru jsou data pru aktualizaci adresních míst RUIAN.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STAV_OK">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Jde o stavovou informaci o tom, že proces předání souborů v předchozím kroku procesu (ADRRUIAN) byl úspěšný. V identifikaci souboru je uveden identifikátor původního souboru.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STAV_CHYBA">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Jde o stavovou informaci o tom, že proces předání souborů v předchozím kroku procesu (ADRRUIAN) nebyl úspěšný. V identifikaci souboru je uveden identifikátor původního souboru.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="OdpovedRozhraniType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Probe - odpověď jednotlivého rozhraní využívaného ISZR (Základní registry, ORG, AISy).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProbeStatus" type="reg:ProbeStatusType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Aplikační status Probe</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NazevRozhrani" type="reg:RozhraniType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Identifikace systému, který poskytuje odpověď, pro rozlišení systémů. Definuje SZR / ISZR při implementaci Probe služby systémem.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="KodISVS" type="reg:KodAisType">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Kód AIS v ISoISVS, pokud je přidělen.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Odpoved">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="CasVydejeOdpovedi" type="reg:CasovaZnackaType">
              <xs:annotation>
                <xs:documentation xml:lang="cs">Systémový čas aplikační logiky při výdeji odpovědi</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="StatusDat" type="xs:boolean">
              <xs:annotation>
                <xs:documentation xml:lang="cs">false - nepodařilo se načíst data, true - databáze je funkční</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="VariabilniInformace" type="reg:VariabilniInformaceType">
              <xs:annotation>
                <xs:documentation xml:lang="cs">Variabilní informace definovaná poskytujícím systémem (např. počet změn, náhodná hodnota pro omezení cache a pod.)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="SeznamRozhraniType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Seznam testovaných rozhraní využívaných ISZR (Základní registry, ORG, AISy), např. "ROB ORQ AISEO".</xs:documentation>
    </xs:annotation>
    <xs:list itemType="reg:RozhraniType" />
  </xs:simpleType>
  <xs:simpleType name="RozhraniType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Názvy rozhraní využívaných ISZR (Základní registry, ORG, AISy).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="reg:NonEmptyNormStringType" />
  </xs:simpleType>
  <xs:complexType name="ProbeStatusType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Aplikační status pro jednotlivá rozhraní využívaná ISZR (Základní registry, ORG, AISy).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="VysledekProbeKodType" type="reg:VysledekKodType" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="VysledekDetail">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="VysledekSubKod" type="reg:VysledekSubKodProbeType" />
            <xs:element minOccurs="0" name="VysledekPopis" type="reg:VysledekPopisType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="VariabilniInformaceType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Variabilní informace, která se přidává do odpovědi služby Probe (obsah definuje poskytující systém)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:long" />
  </xs:simpleType>
  <xs:simpleType name="VysledekSubKodProbeType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Detailní kód výsledku, výčtový typ.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="OK">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Systém je plně funkční.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NEVALIDNI DATA">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Aplikační data nejsou validní podle XSD dokumentů. Odpověď volaného systému.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NEVALIDNI ZADOST">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Kód služby neodpovídá XML struktuře žádosti. Odpoěď volaného systému.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CHYBA DATABAZE">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Chyba databáze systému, systém není schopen zpracovávat data.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ROZHRANI NEDOSTUPNE">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Testovaný systém (ZR, ORG, AISy) není dostupný - chyba volání. Odpověď vytváří ISZR - nedostalo odpověď.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NEVALIDNI ODPOVED">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Testovaný systém (ZR, ORG, AISy) vrací nevalidní odpověď. Odpověď vytváří ISZR - dostalo nevalidní odpověď.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SPECIFIKACE V POPISU">
        <xs:annotation>
          <xs:documentation xml:lang="cs">Detailní popis uveden v popisu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AisKontrolaType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Součet pro kontrolu integrity údajů pro Agendy.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary">
      <xs:length value="64" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="IszrKontrolaType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Součet pro kontrolu integrity údajů pro ISZR.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary" />
  </xs:simpleType>
  <xs:complexType name="LokalniBsiType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Lokální identifikátor BSI. Klíč typu integer.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:int" />
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="GlobalniBsiType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Bezvýznamový směrový identifikátor pro elektronickou identifikaci. Pro rozšíření o potřebné atributy.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="reg:BsiType" />
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="PrevodBsiType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Převodník mezi lokálními a globálními BSI.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LokalniBsi" type="reg:LokalniBsiType" />
      <xs:element minOccurs="0" name="GlobalniBsi" type="reg:GlobalniBsiType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MapaBsiType">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Seznam všech BSI převodníků.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="PrevodBsi" type="reg:PrevodBsiType" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
</xs:schema>