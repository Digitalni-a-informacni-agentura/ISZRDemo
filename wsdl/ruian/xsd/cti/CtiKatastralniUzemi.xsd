<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="urn:cz:isvs:ruian:schemas:CtiKatastralniUzemi:v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:cz:isvs:ruian:schemas:CtiKatastralniUzemi:v1" xmlns:ctiKu="urn:cz:isvs:ruian:schemas:CtiKatastralniUzemi:v1" xmlns:base="urn:cz:isvs:ruian:schemas:BaseTypy:v1" xmlns:kuPol="urn:cz:isvs:ruian:schemas:KatastralniUzemiPolozky:v1" xmlns:reg="urn:cz:isvs:reg:schemas:RegTypy:v1" xmlns:kuGml="urn:cz:isvs:ruian:schemas:KatastralniUzemiGML:v1" elementFormDefault="qualified" version="0.40.1">
