            <xs:documentation>Email.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="reg:NonEmptyNormStringType">
            <xs:maxLength value="50"/>
            <xs:pattern value="[^@]+@[^\.]+\..+|"/>
